{
    "manifest_version": 3,
    "name": "GhostClean",
    "description": "Stores URLs and deletes history only for those URLs and their endpoints.",
    "version": "1.0.1",
    "permissions": [
      "history",
      "storage"
    ],
    "icons": {
      "48": "icon/logo.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  